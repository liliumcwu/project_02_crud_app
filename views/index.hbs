<main>
    <div class="form-wrap">
      <div class="row">
        <form class="col s12">
          <div class="row">
            <div class="input-field col s9">
              <input id="card-question" type="text">
              <label for="card-question">Add a question (Ex: What is the value of pi?)</label>
            </div>
            <div class="input-field col s3">
              <input id="card-ans"type="text">
              <label for="card-ans">Write answer (Ex:3.14159)</label>
            </div>
            <button id="submit-button">Submit</button>
          </div>
        </form>
      </div>
    </div>
</main>
