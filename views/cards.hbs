<main>
<div class="row cards">
{{#each newObj}}
  <div class="col s12 m3" id={{_id}}>
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <p class="card-title">Question: {{question}}</>
        <p>Answer: {{answer}}</p>

      </div>
      <div class="card-action">
        <a href="/cards/{{_id}}">Edit Details</a>
        <button type="submit" name="submit_param" value={{_id}} class="link-button-trick" class="delete-button">
            Delete
        </button>
      </div>
    </div>
  </div>
{{/each}}
</div>
</main>
